{
  "$id": "csv-config",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Schema for CSV config files. TODO: Fill out the rest of this schema will all config properties",
  "type": "object",
  "properties": {
    "extractors": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/extractor"
      }
    }
  },
  "$defs": {
    "extractor": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        }
      },
      "required": ["type"]
    }
  },
  "required": ["extractors"]
}
